datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Pet {
  createdAt   DateTime @default(now())
  description String?
  id          String   @id @default(cuid())
  imageUrl    String?
  name        String?
  updatedAt   DateTime @updatedAt
}

model Footer {
  createdAt   DateTime @default(now())
  id          String   @id @default(cuid())
  quotes      String?
  socialLinks Json?
  updatedAt   DateTime @updatedAt
}

model Navigation {
  createdAt DateTime @default(now())
  icon      String?
  id        String   @id @default(cuid())
  name      String?
  updatedAt DateTime @updatedAt
  url       String?
}

model User {
  createdAt DateTime @default(now())
  email     String?  @unique
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
}
